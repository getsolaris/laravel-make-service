        "contract",
        "pattern"
    ],
    "type": "library",
    "require": {
        "php": "^7.1|^8.0",
        "illuminate/support": "~5.6.34|~5.7.0|~5.8.0|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0",
        "illuminate/console": "~5.6.34|~5.7.0|~5.8.0|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5|^10.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "getsolaris",
            "email": "getsolaris.kr@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Getsolaris\\LaravelMakeService\\LaravelMakeServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
